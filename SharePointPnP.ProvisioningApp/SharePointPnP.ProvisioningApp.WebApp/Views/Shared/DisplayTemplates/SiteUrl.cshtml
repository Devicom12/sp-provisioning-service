@model SharePointPnP.ProvisioningApp.WebApp.Models.TemplateParameterModel

<!-- SiteUrl Custom Editor Display -->
<div class="siteUrlLink hide"><a target="_blank" href="@(Model.SPORootSiteUrl + Model.ParameterValue)">@(Model.SPORootSiteUrl + Model.ParameterValue)</a></div>
<div class="provisioningSiteUrlLink show">@(Model.SPORootSiteUrl + Model.ParameterValue)<img width="16" height="16" alt="@Resources.Messages.ProvisioningInProgress" src="data:image/gif;base64," /></div>
<div class="failedSiteUrlLink hide">@(Model.SPORootSiteUrl + Model.ParameterValue)<img width="16" height="16" alt="@Resources.Messages.ProvisioningFailed" src="data:image/gif;base64," /></div>


